<% include /partials/header.ejs %>
<div class="container-fluid">
    <div class="row">
        <div class="col m6 offset-m3">
        <form action="/" method="POST"> 
            <div class="card ">
            <div class="card-content">
                <span class="card-title">Autenticação</span> 
                <div class="row">                                 
                <div class="input-field col s12">
                    <input id="first_name" type="text" class="validate" name="user">
                    <label for="first_name">Usuário</label>
                </div>
                <div class="input-field col s12">
                    <input id="last_name" type="password" class="validate" name="password">
                    <label for="last_name">Senha</label>
                </div>                  
                </div>                  
            </div>
            <div class="card-action">
                <button type="submit" class="btn">Login</button>                  
            </div>
            </div>
        </form>
        </div>                 
    </div>
</div>
<% include ../partials/footer.ejs %>
