<% include ../partials/header.ejs %>
<h3>Editar pessoa</h3>
<p><a class="btn" href="/"><i class="material-icons">arrow_back</i></a></p>
<% results.data.map((contact) => {  %>
  <form method="post" action="/updatePerson/<%= contact.idContact %>">
    <div class="form-group">
      <label for="nome">Name</label>
      <input type="text" id="nome" placeholder="Nome" name="name" value="<%= contact.name %>">
    </div>
    <div class="form-group">
      <label for="nascimento">Phone</label>
      <input type="text" id="nascimento" placeholder="phone" name="phone" value="<%= contact.phone %>">
    </div>
    <button type="submit" class="btn btn-primary">Salvar</button>
  </form>
<% }) %>
<% include ../partials/footer.ejs %>